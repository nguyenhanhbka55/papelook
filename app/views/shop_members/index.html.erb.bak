<h1>Listing shop_members</h1>

<table>
  <thead>
    <tr>      
       
      <th>Staff email</th>     
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shop_members.each do |shop_member| %>
      <tr>        
        
        <td><%= shop_member.staff_email %></td>                           
      </tr>
    <% end %>
  </tbody>
</table>

<br>